{
  "name": "@regenerate/wrapper",
  "version": "1.0.0",
  "description": "Manage side effects with testability and ergonomics",
  "repository": "https://github.com/d-asensio/regenerate.git",
  "author": "David Asensio Ca√±as <david.asensio@icloud.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "examples/*",
    "packages/*"
  ],
  "scripts": {
    "start": "babel-node examples/vanilla/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "standard",
    "lint:fix": "standard --fix",
    "types:check": "yarn workspace @regenerate/jest-matchers run types:check",
    "markdown:generate-toc": "markdown-toc -i ./README.md"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@types/jest": "^27.4.0",
    "babel-jest": "^27.4.6",
    "husky": "^4.2.5",
    "jest": "^27.4.7",
    "jest-extended": "^2.0.0",
    "jest-when": "^3.5.0",
    "lint-staged": ">=10",
    "markdown-toc": "^1.2.0",
    "standard": "^16.0.4",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "README.md": [
      "yarn markdown:generate-toc"
    ],
    "packages/**/*.js": [
      "yarn lint:fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "standard": {
    "parser": "@babel/eslint-parser",
    "env": [
      "jest"
    ]
  },
  "volta": {
    "node": "16.13.2",
    "yarn": "1.22.17"
  },
  "dependencies": {
    "isomorphic-unfetch": "^3.1.0"
  }
}
